
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport"    content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author"      content="Sergey Pozhilov (GetTemplate.com)">

    <title>Onlinelean</title>

    <link rel="shortcut icon" href="{{ asset('super/assets/images/yun.ico') }}">

    <!-- Bootstrap itself -->
    <link href="{{ asset('super/assets/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css">
    <!-- Icons -->
    <link href="{{ asset('super/assets/css/font-awesome.css') }}" rel="stylesheet">

    <!-- Custom styles -->
    <link rel="stylesheet" href="{{ asset('super/assets/css/styles.css') }}">

    <!--[if lt IE 9]> <script src="{{ asset('super/assets/js/html5shiv.js') }}"></script> <![endif]-->
</head>

<body>

<!-- Header -->
<header class="header">
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-push-2 text-center">
                <img src="{{ asset('super/assets/images/logo.png') }}" style="width: 80%;margin-top:20px;">
                {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                    {{ 'layout.logged_in_as'|trans({'%username%': app.user.username}, 'FOSUserBundle') }} |
                    <a href="{{ path('fos_user_security_logout') }}">
                        {{ 'layout.logout'|trans({}, 'FOSUserBundle') }}
                    </a>
                {% else %}

                {% endif %}

                {% for type, messages in app.session.flashbag.all() %}
                    {% for message in messages %}
                        <div class="flash-{{ type }}">
                            {{ message }}
                        </div>
                    {% endfor %}
                {% endfor %}

                <div>
                    {% block fos_user_content %}
                    {% endblock fos_user_content %}
                </div>

            </div>
        </div>

    </div>
</header>
<!-- /Header -->




<!-- JavaScript libs are placed at the end of the document so the pages load faster -->
<script src="{{ asset('super/assets/js/jquery.min.js') }}"></script>
<script src="{{ asset('super/assets/js/template.js') }}"></script>
<script src="{{ asset('super/assets/js/bootstrap.min.js') }}"></script>
<script type="text/javascript">
    $('#myModal').modal(options)
</script>

<style type="text/css">
    .modal-font{
        color: #000;
    }
    .modal-content{
        background: #006699 ;
        background: -moz-linear-gradient(top,  #006699  0%, #0066CC  100%);
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#006699 ), color-stop(100%,#0066CC ));
        background: -webkit-linear-gradient(top,  #006699  0%,#0066CC  100%);
        background: -o-linear-gradient(top,  #006699  0%,#0066CC  100%);
        background: -ms-linear-gradient(top,  #006699  0%,#0066CC  100%);
        background: linear-gradient(to bottom,  #006699  0%,#0066CC  100%);
    }
    .modal-header{
        border-bottom: none;
    }
    .modal-footer{
        border-top: none;
        text-align: center;
    }
</style>
</body>
</html>
